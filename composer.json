{
    "name": "cartalyst/sentinel-social",
    "description": "Social network logins for Sentinel using Oauth and Oauth2, including custom services",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "auth",
        "security",
        "oauth"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cartalyst/sentinel": "~2.0",
        "cartalyst/support": "~1.0",
        "league/oauth1-client": "~1.0",
        "league/oauth2-client": "~1.0",
        "league/oauth2-facebook": "~1.0",
        "league/oauth2-github": "0.2.*",
        "league/oauth2-google": "~1.0",
        "league/oauth2-linkedin": "0.4.*",
        "league/oauth2-instagram": "0.2.*"
    },
    "require-dev": {
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "classmap": [
            "src/Exceptions.php"
        ],
        "psr-4": {
            "Cartalyst\\Sentinel\\Addons\\Social\\": "src/"
        }
    },
    "extra": {
        "component": "addon",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
